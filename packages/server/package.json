{
  "name": "@orpc/server",
  "type": "module",
  "version": "0.0.3",
  "author": {
    "name": "unnoq",
    "email": "contact@unnoq.com",
    "url": "https://unnoq.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/unnoq/unnoq",
  "repository": {
    "type": "git",
    "url": "https://github.com/unnoq/unnoq.git",
    "directory": "examples/typescript-vite-package"
  },
  "keywords": ["unnoq"],
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/src/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      },
      "./fetch": {
        "types": "./dist/src/adapters/fetch.d.ts",
        "import": "./dist/fetch.js",
        "default": "./dist/fetch.js"
      },
      "./ðŸ”’/*": {
        "types": "./dist/src/*.d.ts"
      }
    }
  },
  "exports": {
    ".": "./src/index.ts",
    "./fetch": "./src/adapters/fetch.ts",
    "./ðŸ”’/*": {
      "types": "./src/*.ts"
    }
  },
  "files": ["dist", "src"],
  "scripts": {
    "build": "tsup --entry.index=src/index.ts --entry.fetch=src/adapters/fetch.ts --clean --sourcemap --splitting --format=esm --onSuccess='tsc -b --noCheck'",
    "type:check": "tsc -b"
  },
  "dependencies": {
    "@orpc/contract": "workspace:*",
    "@orpc/shared": "workspace:*",
    "@orpc/transformer": "workspace:*"
  },
  "peerDependencies": {
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "hono": "^4.6.3"
  }
}
